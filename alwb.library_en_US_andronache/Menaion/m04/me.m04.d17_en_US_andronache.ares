meMA.Ode1C1H.ode = misc_en_US_andronache.Ode1
meMA.Ode1C1H.mode = misc_en_US_andronache.Mode5
meMA.Ode1C1H.melody = he.h.m5_en_US_andronache.IpponKaiAnavatin.melody
meMA.Ode1C1H.incipit = he.h.m5_en_US_andronache.IpponKaiAnavatin.incipit
meMA.Ode1C1H.text = he.h.m5_en_US_andronache.IpponKaiAnavatin.text

meMA.Ode1C11.ode = misc_en_US_andronache.Ode1
meMA.Ode1C11.mode = misc_en_US_andronache.Mode5
meMA.Ode1C11.melody = he.h.m5_en_US_andronache.IpponKaiAnavatin.notmetered
meMA.Ode1C11.incipit = "Having put on the crown…"
meMA.Ode1C11.text = "Having put on the crown of incorruption and standing by the throne of the Lord, all-blessed one, ever save those who faithfully honor your light-bearing contest."

meMA.Ode1C12.ode = misc_en_US_andronache.Ode1
meMA.Ode1C12.mode = misc_en_US_andronache.Mode5
meMA.Ode1C12.melody = he.h.m5_en_US_andronache.IpponKaiAnavatin.notmetered
meMA.Ode1C12.incipit = "You were shown to be…"
meMA.Ode1C12.text = "You were shown to be a precious vessel of the divine Spirit, God-bearing Symeon, and having been anointed by Him, you shepherded His people with the staff of divine doctrines upon the waters of knowledge, O Martyr."

meMA.Ode1C13.ode = misc_en_US_andronache.Ode1
meMA.Ode1C13.mode = misc_en_US_andronache.Mode5
meMA.Ode1C13.melody = he.h.m5_en_US_andronache.IpponKaiAnavatin.notmetered
meMA.Ode1C13.incipit = "You shone in the world…"
meMA.Ode1C13.text = "You shone in the world through word and action, wise one, in brightness of life and shepherded unto a life-bearing pasture a people which honors your divine memory."

meMA.Ode1C1T.ode = misc_en_US_andronache.Ode1
meMA.Ode1C1T.mode = misc_en_US_andronache.Mode5
meMA.Ode1C1T.melody = he.h.m5_en_US_andronache.IpponKaiAnavatin.notmetered
meMA.Ode1C1T.incipit = "Virgin, you bear in your…"
meMA.Ode1C1T.text = "Virgin, you bear in your hands Him who bears all things, and nurse as an infant the provider of milk. Ever entreat Him to take pity on all your people who faithfully hymn you."

meMA.Ode3C1H.ode = misc_en_US_andronache.Ode3
meMA.Ode3C1H.mode = misc_en_US_andronache.Mode5
meMA.Ode3C1H.melody = he.h.m5_en_US_andronache.OPixasEpOudenos.melody
meMA.Ode3C1H.incipit = he.h.m5_en_US_andronache.OPixasEpOudenos.incipit
meMA.Ode3C1H.text = he.h.m5_en_US_andronache.OPixasEpOudenos.text

meMA.Ode3C11.ode = misc_en_US_andronache.Ode3
meMA.Ode3C11.mode = misc_en_US_andronache.Mode5
meMA.Ode3C11.melody = he.h.m5_en_US_andronache.OPixasEpOudenos.notmetered
meMA.Ode3C11.incipit = "Having beautifully ordered the movements…"
meMA.Ode3C11.text = "Having beautifully ordered the movements of your soul and walking the path which leads on high, you turned away from every evil false path and were brought to the gate of life, glorious one, unto everlasting rest."

meMA.Ode3C12.ode = misc_en_US_andronache.Ode3
meMA.Ode3C12.mode = misc_en_US_andronache.Mode5
meMA.Ode3C12.melody = he.h.m5_en_US_andronache.OPixasEpOudenos.notmetered
meMA.Ode3C12.incipit = "Setting your thoughts far from…"
meMA.Ode3C12.text = "Setting your thoughts far from the passions of the body, you offered bloodless sacrifices unto the Lord who was sacrificed for us, and proclaiming His divine incarnation which is beyond words, you were sacrificed as a guileless lamb."

meMA.Ode3C13.ode = misc_en_US_andronache.Ode3
meMA.Ode3C13.mode = misc_en_US_andronache.Mode5
meMA.Ode3C13.melody = he.h.m5_en_US_andronache.OPixasEpOudenos.notmetered
meMA.Ode3C13.incipit = "You were brought up to…"
meMA.Ode3C13.text = "You were brought up to the altar of God as a pure and blameless sacrifice, famous and blessed one, and having been willingly sacrificed for the faith as an acceptable whole-burnt offering, you received illustrious glory, being made illustrious."

meMA.Ode3C1T.ode = misc_en_US_andronache.Ode3
meMA.Ode3C1T.mode = misc_en_US_andronache.Mode5
meMA.Ode3C1T.melody = he.h.m5_en_US_andronache.OPixasEpOudenos.notmetered
meMA.Ode3C1T.incipit = "Pure and ever-blessed one, who…"
meMA.Ode3C1T.text = "Pure and ever-blessed one, who are the adornment of prizewinners and propitiation for sinners, you freed us from the ancestral curse,, having give a body to the Logos who has crowned us all with blessings."

meMA.Ode3Kathisma1.mode = misc_en_US_andronache.Mode1
meMA.Ode3Kathisma1.melody = he.a.m1_en_US_andronache.TonTafonSouSotir.notmetered
meMA.Ode3Kathisma1.incipit = "Wise hierarch Symeon, your holy…"
meMA.Ode3Kathisma1.text = "Wise hierarch Symeon, your holy memory and that of your fellow martyrs arose for us like a sun, enlightening the faithful. Celebrating it today, we cry out: O prizewinners entreat the only lover of humanity on our behalf."

meMA.Ode3KathismaT.mode = misc_en_US_andronache.Mode1
meMA.Ode3KathismaT.melody = he.a.m1_en_US_andronache.TonTafonSouSotir.notmetered
meMA.Ode3KathismaT.incipit = "…"
meMA.Ode3KathismaT.text = //oc.m1.d1_en_US_andronache.ocMA.Kathisma23.text

meMA.Ode3KathismaSt.mode = misc_en_US_andronache.Mode1
meMA.Ode3KathismaSt.melody = he.a.m1_en_US_andronache.TonTafonSouSotir.notmetered
meMA.Ode3KathismaSt.incipit = "Seeing you, O Christ, dead…"
meMA.Ode3KathismaSt.text = "Seeing you, O Christ, dead when stretched upon the cross, your all-blameless mother cried out: My son, co-beginningless with the Father and the Spirit, what is this unutterable economy of yours through which you saved the skilled creation of your spotless hands?" //oc.m1.d4_en_US_andronache.octrMA.Kathisma11.text

meMA.Ode4C1H.ode = misc_en_US_andronache.Ode4
meMA.Ode4C1H.mode = misc_en_US_andronache.Mode5
meMA.Ode4C1H.melody = he.h.m5_en_US_andronache.TinTheianEnnoisas.melody
meMA.Ode4C1H.incipit = he.h.m5_en_US_andronache.TinTheianEnnoisas.incipit
meMA.Ode4C1H.text = he.h.m5_en_US_andronache.TinTheianEnnoisas.text

meMA.Ode4C11.ode = misc_en_US_andronache.Ode4
meMA.Ode4C11.mode = misc_en_US_andronache.Mode5
meMA.Ode4C11.melody = he.h.m5_en_US_andronache.TinTheianEnnoisas.notmetered
meMA.Ode4C11.incipit = "Having been led in bonds…"
meMA.Ode4C11.text = "Having been led in bonds, you destroyed the devices of the evil one, blessed initiate of sacred mysteries, and having contested bravely you received crowns of incorruption."

meMA.Ode4C12.ode = misc_en_US_andronache.Ode4
meMA.Ode4C12.mode = misc_en_US_andronache.Mode5
meMA.Ode4C12.melody = he.h.m5_en_US_andronache.TinTheianEnnoisas.notmetered
meMA.Ode4C12.incipit = "With the spear of your…"
meMA.Ode4C12.text = "With the spear of your words you devoured the compositions of the transgressors, hieromartyr Symeon and you burned the deceit of polytheism by the fire of your blood."

meMA.Ode4C13.ode = misc_en_US_andronache.Ode4
meMA.Ode4C13.mode = misc_en_US_andronache.Mode5
meMA.Ode4C13.melody = he.h.m5_en_US_andronache.TinTheianEnnoisas.notmetered
meMA.Ode4C13.incipit = "Your pains drip healing to…"
meMA.Ode4C13.text = "Your pains drip healing to those with ailing souls, for you were truly shown to walk in the ways of the apostles and a match of the divine prizewinners."

meMA.Ode4C1T.ode = misc_en_US_andronache.Ode4
meMA.Ode4C1T.mode = misc_en_US_andronache.Mode5
meMA.Ode4C1T.melody = he.h.m5_en_US_andronache.TinTheianEnnoisas.notmetered
meMA.Ode4C1T.incipit = "The sun of righteousness, having…"
meMA.Ode4C1T.text = "The sun of righteousness, having arisen from you beyond understanding, enlightened the world and put an end to the winter of deceit, O maiden graced by God."

meMA.Ode5C1H.ode = misc_en_US_andronache.Ode5
meMA.Ode5C1H.mode = misc_en_US_andronache.Mode5
meMA.Ode5C1H.melody = he.h.m5_en_US_andronache.OAnavallomenos.melody
meMA.Ode5C1H.incipit = he.h.m5_en_US_andronache.OAnavallomenos.incipit
meMA.Ode5C1H.text = he.h.m5_en_US_andronache.OAnavallomenos.text

meMA.Ode5C11.ode = misc_en_US_andronache.Ode5
meMA.Ode5C11.mode = misc_en_US_andronache.Mode5
meMA.Ode5C11.melody = he.h.m5_en_US_andronache.OAnavallomenos.notmetered
meMA.Ode5C11.incipit = "You were seen to be…"
meMA.Ode5C11.text = "You were seen to be a pillar and foundation of the Church of Christ by your contest, all-blessed one. Keep it unswerving and unshaken unto the ages by your mediations."

meMA.Ode5C12.ode = misc_en_US_andronache.Ode5
meMA.Ode5C12.mode = misc_en_US_andronache.Mode5
meMA.Ode5C12.melody = he.h.m5_en_US_andronache.OAnavallomenos.notmetered
meMA.Ode5C12.incipit = "With hymns we bless your…"
meMA.Ode5C12.text = "With hymns we bless your way of life, your persecutions, and your resistance, blessed Symeon, by which you were made worthy of the lot of blessedness."

meMA.Ode5C13.ode = misc_en_US_andronache.Ode5
meMA.Ode5C13.mode = misc_en_US_andronache.Mode5
meMA.Ode5C13.melody = he.h.m5_en_US_andronache.OAnavallomenos.notmetered
meMA.Ode5C13.incipit = "Having become a shepherd through…"
meMA.Ode5C13.text = "Having become a shepherd through the myrrh of divine anointing, you were sacrificed for your flock as a fine ram, and made the Church of the first-born rejoice."

meMA.Ode5C1T.ode = misc_en_US_andronache.Ode5
meMA.Ode5C1T.mode = misc_en_US_andronache.Mode5
meMA.Ode5C1T.melody = he.h.m5_en_US_andronache.OAnavallomenos.notmetered
meMA.Ode5C1T.incipit = "Lady, may I, who faithfully…"
meMA.Ode5C1T.text = "Lady, may I, who faithfully glorify you, find you a deliverer from condemnation in the hour in the hour of judgment, and may I be redeemed from places of chastisement which await me."

meMA.Ode6C1H.ode = misc_en_US_andronache.Ode6
meMA.Ode6C1H.mode = misc_en_US_andronache.Mode5
meMA.Ode6C1H.melody = he.h.m5_en_US_andronache.MainomeninKlydoni.melody
meMA.Ode6C1H.incipit = he.h.m5_en_US_andronache.MainomeninKlydoni.incipit
meMA.Ode6C1H.text = he.h.m5_en_US_andronache.MainomeninKlydoni.text

meMA.Ode6C11.ode = misc_en_US_andronache.Ode6
meMA.Ode6C11.mode = misc_en_US_andronache.Mode5
meMA.Ode6C11.melody = he.h.m5_en_US_andronache.MainomeninKlydoni.notmetered
meMA.Ode6C11.incipit = "You were seen to be…"
meMA.Ode6C11.text = "You were seen to be filled with divine joy, blessed Symeon, having received the blessed end through swords with a hundred divine martyrs."

meMA.Ode6C12.ode = misc_en_US_andronache.Ode6
meMA.Ode6C12.mode = misc_en_US_andronache.Mode5
meMA.Ode6C12.melody = he.h.m5_en_US_andronache.MainomeninKlydoni.notmetered
meMA.Ode6C12.incipit = "Young ones and elders, monastics…"
meMA.Ode6C12.text = "Young ones and elders, monastics and sacred ministers, having completed the good fight, you were sacrificed for Christ like lambs."

meMA.Ode6C13.ode = misc_en_US_andronache.Ode6
meMA.Ode6C13.mode = misc_en_US_andronache.Mode5
meMA.Ode6C13.melody = he.h.m5_en_US_andronache.MainomeninKlydoni.notmetered
meMA.Ode6C13.incipit = "Having lived blessedly, O prizewinners…"
meMA.Ode6C13.text = "Having lived blessedly, O prizewinners, you truly obtained a blessed end in Christ, having been perfected by the sword with rejoicing."

meMA.Ode6C1T.ode = misc_en_US_andronache.Ode6
meMA.Ode6C1T.mode = misc_en_US_andronache.Mode5
meMA.Ode6C1T.melody = he.h.m5_en_US_andronache.MainomeninKlydoni.notmetered
meMA.Ode6C1T.incipit = "All-holy one, the Maker of…"
meMA.Ode6C1T.text = "All-holy one, the Maker of the world has risen from you and with rays of divine knowledge He has enlightened those in the night of ignorance."

meMA.Kontakion1.poet = //poets_en_US_andronache.Gerasimou
meMA.Kontakion1.mode = misc_en_US_andronache.Mode4
