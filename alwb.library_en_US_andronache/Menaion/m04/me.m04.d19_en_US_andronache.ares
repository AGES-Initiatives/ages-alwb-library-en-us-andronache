meMA.Ode7C1H.text = he.h.m4_en_US_andronache.ODiasosasEnPyri.text

meMA.Ode7C11.ode = misc_en_US_andronache.Ode7
meMA.Ode7C11.mode = misc_en_US_andronache.Mode4
meMA.Ode7C11.melody = he.h.m4_en_US_andronache.ODiasosasEnPyri.notmetered
meMA.Ode7C11.incipit = "The case of your relics"
meMA.Ode7C11.text = "The case of your relics was given to the faithful as a rod of power against the demons, dispensing plentiful healing to all who cry out, O Paphnutios: O God of our Fathers, you are blessed."

meMA.Ode7C12.ode = misc_en_US_andronache.Ode7
meMA.Ode7C12.mode = misc_en_US_andronache.Mode4
meMA.Ode7C12.melody = he.h.m4_en_US_andronache.ODiasosasEnPyri.notmetered
meMA.Ode7C12.incipit = "Piously celebrating your all-illustrious day,"
meMA.Ode7C12.text = "Piously celebrating your all-illustrious day, O prize-winner, we, born on earth, cry to the Lord: greatly-hymned Lord, the God of our Fathers, you are blessed."

meMA.Ode7C13.ode = misc_en_US_andronache.Ode7
meMA.Ode7C13.mode = misc_en_US_andronache.Mode4
meMA.Ode7C13.melody = he.h.m4_en_US_andronache.ODiasosasEnPyri.notmetered
meMA.Ode7C13.incipit = "Now holding the case of"
meMA.Ode7C13.text = "Now holding the case of your relics as a treasure, the Church of Christ shouts rejoicing, O Paphnutios: greatly-hymned Lord, the God of our Fathers, you are blessed."

meMA.Ode7C1T.ode = misc_en_US_andronache.Ode7
meMA.Ode7C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode7C1T.melody = he.h.m4_en_US_andronache.ODiasosasEnPyri.notmetered
meMA.Ode7C1T.incipit = "Let us, the faithful, venerate"
meMA.Ode7C1T.text = "Let us, the faithful, venerate  as the blessed mother of the Lord her who, in a manner past expressing and beyond nature, received the unchangeable God in her womb to be in the company of mortals in his compassion."

meMA.Ode8C1H.ode = misc_en_US_andronache.Ode8
meMA.Ode8C1H.mode = misc_en_US_andronache.Mode4
meMA.Ode8C1H.melody = misc_en_US_andronache.Heirmos
meMA.Ode8C1H.incipit = he.h.m4_en_US_andronache.TonEnMorfiAngelou.incipit
meMA.Ode8C1H.text = he.h.m4_en_US_andronache.TonEnMorfiAngelou.text

meMA.Ode8C11.ode = misc_en_US_andronache.Ode8
meMA.Ode8C11.mode = misc_en_US_andronache.Mode4
meMA.Ode8C11.melody = he.h.m4_en_US_andronache.TonEnMorfiAngelou.notmetered
meMA.Ode8C11.incipit = "Now the bright day of"
meMA.Ode8C11.text = "Now the bright day of the prize-winner’s memory flashes light-bearing and most bright. Celebrating it with yearning, we cry out: O people greatly exalt Christ unto the ages."

meMA.Ode8C12.ode = misc_en_US_andronache.Ode8
meMA.Ode8C12.mode = misc_en_US_andronache.Mode4
meMA.Ode8C12.melody = he.h.m4_en_US_andronache.TonEnMorfiAngelou.notmetered
meMA.Ode8C12.incipit = "Standing in the stadium, you"
meMA.Ode8C12.text = "Standing in the stadium, you contended youthfully and received trophies of victory, all-blessed one, unceasingly making melody to Christ the Master and magnifying Him unto the ages."

meMA.Ode8C13.ode = misc_en_US_andronache.Ode8
meMA.Ode8C13.mode = misc_en_US_andronache.Mode4
meMA.Ode8C13.melody = he.h.m4_en_US_andronache.TonEnMorfiAngelou.notmetered
meMA.Ode8C13.incipit = "How truly beautiful were you"
meMA.Ode8C13.text = "How truly beautiful were you shown in many struggles, O hieromartyr, having been robed with a sacred robe made red by your blood, and receiving from God a crown unto the ages."

meMA.Ode8C1T.ode = misc_en_US_andronache.Ode8
meMA.Ode8C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode8C1T.melody = he.h.m4_en_US_andronache.TonEnMorfiAngelou.notmetered
meMA.Ode8C1T.incipit = "Let us hymn unto the"
meMA.Ode8C1T.text = "Let us hymn unto the ages the only Theotokos, who without seed conceived in her womb and gave birth to Christ the only Redeemer and Savior for us who had suffered the ill-will of the evil foe."

meMA.Ode9C1H.ode = misc_en_US_andronache.Ode9
meMA.Ode9C1H.mode = misc_en_US_andronache.Mode4
meMA.Ode9C1H.melody = misc_en_US_andronache.Heirmos
meMA.Ode9C1H.incipit = he.h.m4_en_US_andronache.EvaMenToTisParakois.incipit
meMA.Ode9C1H.text = he.h.m4_en_US_andronache.EvaMenToTisParakois.text

meMA.Ode9C11.ode = misc_en_US_andronache.Ode9
meMA.Ode9C11.mode = misc_en_US_andronache.Mode4
meMA.Ode9C11.melody = he.h.m4_en_US_andronache.EvaMenToTisParakois.notmetered
meMA.Ode9C11.incipit = "Invincible, all-praised prize-winner, you bestow"
meMA.Ode9C11.text = "Invincible, all-praised prize-winner, you bestow abundant grace to all who faithfully come to your divine temple and sing hymns to the Savior and Lord, and celebrate your memory. "

meMA.Ode9C12.ode = misc_en_US_andronache.Ode9
meMA.Ode9C12.mode = misc_en_US_andronache.Mode4
meMA.Ode9C12.melody = he.h.m4_en_US_andronache.EvaMenToTisParakois.notmetered
meMA.Ode9C12.incipit = "Importune the Lord by your"
meMA.Ode9C12.text = "Importune the Lord by your all-sacred mediation to be gracious to all the faithful Orthodox who from everywhere run to observe your divine festival on your memorial."

meMA.Ode9C13.ode = misc_en_US_andronache.Ode9
meMA.Ode9C13.mode = misc_en_US_andronache.Mode4
