meMA.Ode4C11.ode = misc_en_US_andronache.Ode4
meMA.Ode4C11.mode = misc_en_US_andronache.Mode4
meMA.Ode4C11.melody = he.h.m4_en_US_andronache.TousOuranousIAretiSou.notmetered
meMA.Ode4C11.incipit = "We all now celebrate with"
meMA.Ode4C11.text = "We all now celebrate with gladness, dancing together with angels and archangels, observing your illustrious memory, prize-winner Paphnutios."

meMA.Ode4C12.ode = misc_en_US_andronache.Ode4
meMA.Ode4C12.mode = misc_en_US_andronache.Mode4
meMA.Ode4C12.melody = he.h.m4_en_US_andronache.TousOuranousIAretiSou.notmetered
meMA.Ode4C12.incipit = "You have been truly glorified"
meMA.Ode4C12.text = "You have been truly glorified by divine grace and you now stand with angels in the presence of the Master. Ever remember us, prize-winning martyr Paphnutios."

meMA.Ode4C13.ode = misc_en_US_andronache.Ode4
meMA.Ode4C13.mode = misc_en_US_andronache.Mode4
meMA.Ode4C13.melody = he.h.m4_en_US_andronache.TousOuranousIAretiSou.notmetered
meMA.Ode4C13.incipit = "You endured hardships and pain,"
meMA.Ode4C13.text = "You endured hardships and pain, blessed one, with a calm and brave spirit and truly received the crowns from the hand of the King of all"

meMA.Ode4C1T.ode = misc_en_US_andronache.Ode4
meMA.Ode4C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode4C1T.melody = he.h.m4_en_US_andronache.TousOuranousIAretiSou.notmetered
meMA.Ode4C1T.incipit = "Being greater than the ranks"
meMA.Ode4C1T.text = "Being greater than the ranks of Cherubim and having God in the flesh riding in your embraces, rejoice, unwedded bride."

meMA.Ode5C1H.ode = misc_en_US_andronache.Ode5
meMA.Ode5C1H.mode = misc_en_US_andronache.Mode4
meMA.Ode5C1H.melody = misc_en_US_andronache.Heirmos
meMA.Ode5C1H.incipit = he.h.m4_en_US_andronache.OAnateilasToFos.incipit
meMA.Ode5C1H.text = he.h.m4_en_US_andronache.OAnateilasToFos.text

meMA.Ode5C11.ode = misc_en_US_andronache.Ode5
meMA.Ode5C11.mode = misc_en_US_andronache.Mode4
meMA.Ode5C11.melody = he.h.m4_en_US_andronache.OAnateilasToFos.notmetered
meMA.Ode5C11.incipit = "You who have shown us"
meMA.Ode5C11.text = "You who have shown us the memory of the brave one brighter than the sun: glory to you. Glory to you, Jesus, Son of God."

meMA.Ode5C12.ode = misc_en_US_andronache.Ode5
meMA.Ode5C12.mode = misc_en_US_andronache.Mode4
meMA.Ode5C12.melody = he.h.m4_en_US_andronache.OAnateilasToFos.notmetered
meMA.Ode5C12.incipit = "With most sacred hearts and"
meMA.Ode5C12.text = "With most sacred hearts and faithful spirit, we hymn your memory, glorious martyr, magnifying Christ."

meMA.Ode5C13.ode = misc_en_US_andronache.Ode5
meMA.Ode5C13.mode = misc_en_US_andronache.Mode4
meMA.Ode5C13.melody = he.h.m4_en_US_andronache.OAnateilasToFos.notmetered
meMA.Ode5C13.incipit = "You were not at all"
meMA.Ode5C13.text = "You were not at all afraid of sword and beast, and fire, and death, martyr, crying: Glory to you, Jesus, Son of God."

meMA.Ode5C1T.ode = misc_en_US_andronache.Ode5
meMA.Ode5C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode5C1T.melody = he.h.m4_en_US_andronache.OAnateilasToFos.notmetered
meMA.Ode5C1T.incipit = "Creation unceasingly offers you, O"
meMA.Ode5C1T.text = "Creation unceasingly offers you, O Virgin, the saying of the angel: Rejoice, you who bore Jesus, the Logos of God."

meMA.Ode6C1H.ode = misc_en_US_andronache.Ode6
meMA.Ode6C1H.mode = misc_en_US_andronache.Mode4
meMA.Ode6C1H.melody = misc_en_US_andronache.Heirmos
meMA.Ode6C1H.incipit = he.h.m4_en_US_andronache.EvoiseProtypon.incipit
meMA.Ode6C1H.text = he.h.m4_en_US_andronache.EvoiseProtypon.text

meMA.Ode6C11.ode = misc_en_US_andronache.Ode6
meMA.Ode6C11.mode = misc_en_US_andronache.Mode4
meMA.Ode6C11.melody = he.h.m4_en_US_andronache.EvoiseProtypon.notmetered
meMA.Ode6C11.incipit = "The grace which has been"
meMA.Ode6C11.text = "The grace which has been given to you, all-glorious prize-winner, has been poured out to the ends of the world. Therefore, we all magnify Christ, the giver, with faith."

meMA.Ode6C12.ode = misc_en_US_andronache.Ode6
meMA.Ode6C12.mode = misc_en_US_andronache.Mode4
meMA.Ode6C12.melody = he.h.m4_en_US_andronache.EvoiseProtypon.notmetered
meMA.Ode6C12.incipit = "Paphnutios, the greatest servant of"
meMA.Ode6C12.text = "Paphnutios, the greatest servant of the Trinity, has been shown to be fearsome to the adverse powers, driving them away and illuminating the flocks of the faithful"

meMA.Ode6C13.ode = misc_en_US_andronache.Ode6
meMA.Ode6C13.mode = misc_en_US_andronache.Mode4
meMA.Ode6C13.melody = he.h.m4_en_US_andronache.EvoiseProtypon.notmetered
meMA.Ode6C13.incipit = "Christ glorified you with wonders"
meMA.Ode6C13.text = "Christ glorified you with wonders and signs, O prize-winner, and showed you to all as a healer, all-blessed Paphnutios. Therefore we honor your memory."

meMA.Ode6C1T.ode = misc_en_US_andronache.Ode6
meMA.Ode6C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode6C1T.melody = he.h.m4_en_US_andronache.EvoiseProtypon.notmetered
meMA.Ode6C1T.incipit = "Bride of God and Theotokos,"
meMA.Ode6C1T.text = "Bride of God and Theotokos, importune our God who took flesh from you for those who entreat you that they all be delivered from the varied temptations of the serpent"

