meMA.Ode1C11.ode = misc_en_US_andronache.Ode1
meMA.Ode1C11.mode = misc_en_US_andronache.Mode4
meMA.Ode1C11.melody = he.h.m4_en_US_andronache.ThalassisToErythraion.notmetered
meMA.Ode1C11.incipit = "Being aflame with reverence you"
meMA.Ode1C11.text = "Being aflame with reverence you followed in the footsteps of God who has appeared in the flesh that you may become rich from that whose name you fittingly bore, given beforehand through foreknowledge."

meMA.Ode1C12.ode = misc_en_US_andronache.Ode1
meMA.Ode1C12.mode = misc_en_US_andronache.Mode4
meMA.Ode1C12.melody = he.h.m4_en_US_andronache.ThalassisToErythraion.notmetered
meMA.Ode1C12.incipit = "Being aflame with reverence you"
meMA.Ode1C12.text = "Being aflame with reverence you followed in the footsteps of God who has appeared in the flesh that you may become rich from that whose name you fittingly bore, given beforehand through foreknowledge."

meMA.Ode1C13.ode = misc_en_US_andronache.Ode1
meMA.Ode1C13.mode = misc_en_US_andronache.Mode4
meMA.Ode1C13.melody = he.h.m4_en_US_andronache.ThalassisToErythraion.notmetered
meMA.Ode1C13.incipit = "Leo, the tyrant, rushing headlong"
meMA.Ode1C13.text = "Leo, the tyrant, rushing headlong like a beast, pursued the chosen ones, not bearing to see them honoring the icon of Christ. With them, he also sentenced you, Theophanes, to exile."

meMA.Ode1C1T.ode = misc_en_US_andronache.Ode1
meMA.Ode1C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode1C1T.melody = he.h.m4_en_US_andronache.ThalassisToErythraion.notmetered
meMA.Ode1C1T.incipit = "Seedlessly, by the will of"
meMA.Ode1C1T.text = "Seedlessly, by the will of the Father, you conceived from the divine Spirit the Son of God and gave birth in the flesh to is from the Father without a mother and, for us, from you without a father."

meMA.Ode3C1H.ode = misc_en_US_andronache.Ode3
meMA.Ode3C1H.mode = misc_en_US_andronache.Mode4
meMA.Ode3C1H.melody = misc_en_US_andronache.Heirmos
meMA.Ode3C1H.incipit = he.h.m4_en_US_andronache.OukEnSofia.incipit
meMA.Ode3C1H.text = he.h.m4_en_US_andronache.OukEnSofia.text

meMA.Ode3C11.ode = misc_en_US_andronache.Ode3
meMA.Ode3C11.mode = misc_en_US_andronache.Mode4
meMA.Ode3C11.melody = he.h.m4_en_US_andronache.OukEnSofia.notmetered
meMA.Ode3C11.incipit = "You were seen to be"
meMA.Ode3C11.text = "You were seen to be filled with divine light, O Theophanes, having been united through love to the one for whom you yearned. Therefore we honor your glorious memory"

meMA.Ode3C12.ode = misc_en_US_andronache.Ode3
meMA.Ode3C12.mode = misc_en_US_andronache.Mode4
meMA.Ode3C12.melody = he.h.m4_en_US_andronache.OukEnSofia.notmetered
meMA.Ode3C12.incipit = "Having given up the impious"
meMA.Ode3C12.text = "Having given up the impious teachings of Leo, you most piously strengthened the precepts of the Church. Therefore Christ glorified you with miracles."

meMA.Ode3C13.ode = misc_en_US_andronache.Ode3
meMA.Ode3C13.mode = misc_en_US_andronache.Mode4
meMA.Ode3C13.melody = he.h.m4_en_US_andronache.OukEnSofia.notmetered
meMA.Ode3C13.incipit = "Venerable one, your soul, fortified"
meMA.Ode3C13.text = "Venerable one, your soul, fortified through hope and faith, strengthens with firmness the weakness of the flesh, making the body, its fellow-servant, a suitable dwelling for God."

meMA.Ode3C1T.ode = misc_en_US_andronache.Ode3
meMA.Ode3C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode3C1T.melody = he.h.m4_en_US_andronache.OukEnSofia.notmetered
meMA.Ode3C1T.incipit = "You, who gave birth to"
meMA.Ode3C1T.text = "You, who gave birth to God in the flesh without a husband, strengthen me who am harassed by the attacks of the passions, for I have no other help but you, pure one."

meMA.Ode3Kathisma1.mode = misc_en_US_andronache.Mode3
meMA.Ode3Kathisma1.melody = he.a.m3_en_US_andronache.TheiasPisteos.notmetered
meMA.Ode3Kathisma1.incipit = "Having gained divine zeal, you"
meMA.Ode3Kathisma1.text = "Having gained divine zeal, you loathed ungodly teaching and battled many kinds of dangers, being sent unrighteously into exile and you were piously perfected, all-blessed Father Theophanes; Entreat Christ God that great mercy be granted to us."

meMA.Ode3KathismaT.mode = misc_en_US_andronache.Mode3
meMA.Ode3KathismaT.melody = he.a.m3_en_US_andronache.TheiasPisteos.notmetered
