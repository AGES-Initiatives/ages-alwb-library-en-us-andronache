meMA.Ode9C11.ode = misc_en_US_andronache.Ode9
meMA.Ode9C11.mode = misc_en_US_andronache.Mode4
meMA.Ode9C11.melody = he.h.m4_en_US_andronache.LithosAcheirotmitos.notmetered
meMA.Ode9C11.incipit = "A word going forth from…"
meMA.Ode9C11.text = "A word going forth from your divine lips, O you who speak about God, makes sweet the minds of the pious faithful by grace, like a honeycomb dripping the comeliness of understandings."

meMA.Ode9C12.ode = misc_en_US_andronache.Ode9
meMA.Ode9C12.mode = misc_en_US_andronache.Mode4
meMA.Ode9C12.melody = he.h.m4_en_US_andronache.LithosAcheirotmitos.notmetered
meMA.Ode9C12.incipit = "O God-inspired one, you walk…"
meMA.Ode9C12.text = "O God-inspired one, you walk about within the innermost heavenly places around the Lord and King of all, where the angelic luminaries and the ranks of the saints dance."

meMA.Ode9C13.ode = misc_en_US_andronache.Ode9
meMA.Ode9C13.mode = misc_en_US_andronache.Mode4
meMA.Ode9C13.melody = he.h.m4_en_US_andronache.LithosAcheirotmitos.notmetered
meMA.Ode9C13.incipit = "Seeking the life of the…"
meMA.Ode9C13.text = "Seeking the life of the angels while upon the earth, Father, you were made worthy of the angelic blessedness in heaven, entreat Christ there that all who hymn you be saved."

meMA.Ode9C14.ode = misc_en_US_andronache.Ode9
meMA.Ode9C14.mode = misc_en_US_andronache.Mode4
meMA.Ode9C14.melody = he.h.m4_en_US_andronache.LithosAcheirotmitos.notmetered
meMA.Ode9C14.incipit = "As bowls of aromatic herbs…"
meMA.Ode9C14.text = "As bowls of aromatic herbs, your jaws, godly-minded one, you were seen as an alabaster of wisdom of the nard of most precious faith, spicing up the precious body of Christ with your teachings."

meMA.Ode9C1T.ode = misc_en_US_andronache.Ode9
meMA.Ode9C1T.mode = misc_en_US_andronache.Mode4
meMA.Ode9C1T.melody = he.h.m4_en_US_andronache.LithosAcheirotmitos.notmetered
meMA.Ode9C1T.incipit = "Isaiah saw you, O Virgin…"
meMA.Ode9C1T.text = "Isaiah saw you, O Virgin, as a light cloud, for coming upon you, the Lord destroyed all things made by hand and revealed the knowledge of himself to those who hymn you."

